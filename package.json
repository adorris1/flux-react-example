    "debug": "^2.1.1",
    "express": "^4.11.1",
    "express-error-handler": "git://github.com/localnerve/express-error-handler",
    "flux-router-component": "^0.5.6",
    "fluxible": "^0.2.0",
    "fluxible-plugin-fetchr": "^0.2.2",
    "fluxible-plugin-dynamic-routr": "^0.1.0",
